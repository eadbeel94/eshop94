{
  "plugins": ["jsdoc-http-plugin"],
  "source": {
    "include": ["../backend/functions"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": "true",
    "destination": "./doc/backend",
    "template": "node_modules/docdash",
    "readme": "./README.b.md"
  }
}